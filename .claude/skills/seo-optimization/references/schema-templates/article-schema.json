{
  "@context": "https://schema.org",
  "@type": "Article",
  "headline": "{title}",
  "description": "{meta_description}",
  "image": "{featured_image_url}",
  "author": {
    "@type": "Person",
    "name": "{author_name}"
  },
  "publisher": {
    "@type": "Organization",
    "name": "{brand_name}",
    "logo": {
      "@type": "ImageObject",
      "url": "{logo_url}"
    }
  },
  "datePublished": "{publish_date}",
  "dateModified": "{modified_date}",
  "mainEntityOfPage": {
    "@type": "WebPage",
    "@id": "{page_url}"
  }
}
