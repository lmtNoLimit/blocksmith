# Google Gemini API (optional, falls back to mock if not set)
GEMINI_API_KEY=your_gemini_key

# Shopify Configuration (automatically set by Shopify CLI)
# SHOPIFY_API_KEY=your_api_key
# SHOPIFY_API_SECRET=your_api_secret
# SHOPIFY_APP_URL=https://your-app.com

# Billing Configuration
# Set to "true" to use Shopify test mode (free subscriptions on development stores)
# Set to "false" for production billing
BILLING_TEST_MODE=true

# Encryption key for sensitive data (storefront passwords)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
# REQUIRED for native preview with password-protected storefronts
ENCRYPTION_KEY=

# Database
DATABASE_URL=
